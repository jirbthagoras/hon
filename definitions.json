{
     "rabbit_version": "3.13.0",
     "users": {
          "name": "guest",
          "password": "guest",
          "tags": "administrator"
     },
     "vhosts": [ { "name": "/" } ],
     "permissions": [
          {
               "user": "guest",
               "vhost": "/",
               "configure": ".*",
               "write": ".*",
               "read": ".*"
          }
     ],
     "exchanges": [
          {
               "name": "goal_exchange",
               "vhost": "/",
               "type": "x-delayed-message",
               "durable": true,
               "auto_delete": false,
               "internal": false,
               "arguments": {
                    "x-delayed-type": "direct"
               }
          }
     ],
     "queues": [
          {
               "name": "goal_queue",
               "vhost": "/",
               "durable": true,
               "auto_delete": false,
               "arguments": {}
          },
          {
               "name": "deadline_queue",
               "vhost": "/",
               "durable": true,
               "auto_delete": false,
               "arguments": {}
          }
     ],
     "bindings": [
    {
      "source": "goal_exchange",
      "vhost": "/",
      "destination": "goal_queue",
      "destination_type": "queue",
      "routing_key": "goal",
      "arguments": {}
    },
    {
      "source": "goal_exchange",
      "vhost": "/",
      "destination": "deadline_queue",
      "destination_type": "queue",
      "routing_key": "deadline",
      "arguments": {}
    }
  ]
}